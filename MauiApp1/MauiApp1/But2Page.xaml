<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp1.But2Page"
             Title="Button 2 Page">
    <VerticalStackLayout>
        <Label 
            Text="Button 2 Page"
            VerticalOptions="Center"  Margin="30" FontSize="Large"
            HorizontalOptions="Center" />

        <Button Text="Go Back" Clicked="GoBackButton_Clicked" Margin="10, 30, 10, 10" />
    
        <Button Text="Go To But2 Sub" Clicked="GoBut2Sub_Clicked" Margin="10, 30, 10, 10" />

        <Button Text="Go To But2 Sub Sub" Clicked="GoBut2SubSub_Clicked" Margin="10, 30, 10, 10" />
    
    </VerticalStackLayout>
</ContentPage>